{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~components": ["src/components"],
      "~interfaces": ["src/interfaces"],
      "~navigators": ["src/navigators"],
      "~screens": ["src/screens"],
      "~assets/*": ["src/assets/*"],
      "~store": ["src/store"],
      "~hooks": ["src/hooks"],
      "~utils": ["src/utils"]
    }
  }
}
